<div class="container-fluid wide my-5 pb-md-5">
    <div class="container-fluid row m-0 p-0">
        <div class="col-12 col-md-9 mb-auto p-0 mx-auto pr-md-4 bookingContainer row">
            <div class="container bookingContainer1 bg-white col-12 col-md-6 border-right p-3">
                <small class="text-muted">Regina Pinherio</small>
                <p class="header" id="SOIFormTopic">60 Minute Slot</p>
                <small class="text-muted font-secondary mt-2" style="letter-spacing: 0.05rem;"><i class="far fa-clock">&nbsp;&nbsp;&nbsp;</i>1 hr</small><br>
                <small class="text-muted font-secondary mt-2 SOISecondayInfo" id="SOISecondayInfo1" style="letter-spacing: 0.05rem;"><i class="far fa-calendar-check"></i>&nbsp;&nbsp;&nbsp;<span id="SOISecondayInfoDateTime"></span></small>
                <small class="text-muted font-secondary mt-2 SOISecondayInfo" id="SOISecondayInfo2" style="letter-spacing: 0.05rem;"><i class="fas fa-globe-asia"></i>&nbsp;&nbsp;&nbsp;Indian Standard Time</small><br>
            </div>
            <div class="container bookingContainer2 bg-white col-12 col-md-6 py-4 px-4">
                <p class="header" id="SOIFormTitle">Choose a Date and Time</p>
                <br>
                <%- include ('./partials/messages') %>  
                <form action="/formHandling/servicesForm" id="appointmentsForm" method="POST">
                    <div class="container-fluid m-0 p-0" id="SOI-firstForm">
                        <div class="form-group">
                            <label for="exampleInputName1">Date:</label>
                            <input type="date" class="form-control shadow-none" id="userSelectedDate" name="userDate" aria-describedby="" autofocus required>
                        </div>
                        <div class="form-group">
                            <label for="exampleInputName1">Time:</label>
                            <!-- <input type="time" class="form-control" id="userSelectedTime" name="userTime" aria-describedby=""> -->
                            <select id="userSelectedTimeSlot" class="form-control shadow-none" name="TimeSlot" required>
                            <option class="pt-5" disabled selected>select...</option>
                            <option class="pt-5" value="09:00AM - 10:00AM">09:00 AM</option>
                            <option class="pt-5" value="10:00AM - 11:00AM">10:00 AM</option>
                            <option class="pt-5" value="11:00AM - 12:00PM">11:00 AM</option>
                            <option class="pt-5" value="12:00PM - 01:00PM">12:00 AM</option>
                            <option class="pt-5" value="01:00PM - 02:00PM" disabled>01:00 AM</option>
                            <option class="pt-5" value="02:00PM - 03:00PM">02:00 PM</option>
                            <option class="pt-5" value="03:00PM - 04:00PM">03:00 PM</option>
                            <option class="pt-5" value="04:00PM - 05:00PM">04:00 PM</option>
                            <option class="pt-5" value="05:00PM - 06:00PM">05:00 PM</option>
                            <option class="pt-5" value="06:00PM - 07:00PM">06:00 PM</option>
                            <option class="pt-5" value="07:00PM - 08:00PM">07:00 PM</option>
                            <option class="pt-5" value="08:00PM - 09:00PM">08:00 PM</option>
                            <option class="pt-5" value="09:00PM - 10:00PM">09:00 PM</option>
                            </select>
                        </div>
                        <br>
                    </div>
                    <div class="container-fluid m-0 p-0 SOISecondayInfo" id="SOI-secondForm">
                        <div class="form-group">
                            <label for="exampleInputName1">Name:</label>
                            <input type="text" class="form-control shadow-none" id="userName" name="userName" aria-describedby="">
                          </div>
                          <div class="form-group">
                            <label for="exampleInputName1">Phone No.:</label>
                            <input type="text" class="form-control shadow-none" id="userPhone" name="userPhone" aria-describedby="">
                          </div>
                          <div class="form-group">
                            <label for="exampleInputEmail1">Email address:</label>
                            <input type="email" class="form-control shadow-none" id="userEmail" name="userEmail"  aria-describedby="emailHelp">
                            <small id="emailHelp" class="form-text text-muted">We'll never share your email or phone details with anyone else.</small>
                          </div>
                    </div>
                    <div class="container-fluid m-0 p-0 SOISecondayInfo" id="SOI-thirdForm">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Service of Interest:</label>
                            <select id="inputState" value="" class="form-control shadow-none" name="SOI">
                              <option disabled selected>Choose...</option>
                              <option id="diet-prescription" value="diet-prescription">Diet Prescription</option>
                              <option id="preventive-diet" value="preventive-diet">Preventive Diet</option>
                              <option id="workshops" value="workshops">Workshops</option>
                              <option id="sports-nutrition" value="sports-nutrition">Sports Nutrition</option>
                              <option id="lifestyle-modification" value="lifestyle-modification">Lifestyle Modification</option>
                              <option id="custom-dietplans" value="custom-dietplans">Customized Diet Plans</option>
                              <option id="healthrisk-accessment" value="healthrisk-accessment">Health Risk Assessment</option>
                              <option id="nutritional-counselling" value="nutritional-counselling">Nutritional Counselling</option>
                            </select>
                        </div>
                        <div class="form-group mt-4">
                          <label for="exampleInputEmail1">Please share anything that will help me prepare for our meeting:</label>
                          <textarea class="form-control shadow-none" id="userMessage" name="userMessage"></textarea>
                        </div>
                    </div>
                    <div class="container-fluid m-0 p-0 SOISecondayInfo" id="SOI-fourthForm">
                        <div class="form-group">
                            <label class="text-muted" for="exampleInputName1">Date.:</label> <span class="" id="confirmDate"></span><br>
                            <label class="text-muted" for="exampleInputName1">Time.:</label> <span class="" id="confirmTime"></span><br>
                            <label class="text-muted" for="exampleInputName1">Phone No.:</label> <span class="" id="confirmPhoneNo"></span><br>
                            <label class="text-muted" for="exampleInputName1">Email Id.:</label> <span class="" id="confirmEmail"></span><br>
                            <label class="text-muted" for="exampleInputName1">Payment.:</label> <span class="" id="confirmPrice">&#8377;1500</span><br>
                        </div>
                    </div>
    
                </form>        
                <!-- first form -->
                <button onclick="timeslotConfirmation()" class="btn text-light font-weight-bold float-right mt-4 shadow-none w-50 servicesBooking-ConfirmBtn" id="SOI-firstConfirm">Next</button>
                <!-- second form -->
                <button onclick="personalDetailsBack()" class="btn float-left mt-4 shadow-none btn-secondary border-0 w-25 bg-white text-dark shadow-lg SOISecondayInfo servicesBooking-BackBtn" id="SOI-firstBack">Back</button>
                <button onclick="onPersonalDetailsConfirmation()" class="btn text-light font-weight-bold float-right mt-4 shadow-none w-50 SOISecondayInfo servicesBooking-ConfirmBtn"  id="SOI-secondConfirm">Next</button>
                <!-- third form btns -->
                <button onclick="meetingDetailsBack()" class="btn float-left mt-4 shadow-none btn-secondary border-0 w-25 bg-white text-dark shadow-lg SOISecondayInfo servicesBooking-BackBtn" id="SOI-secondBack">Back</button>
                <button onclick="onMeetingDetailsConfirmation()" class="btn text-light font-weight-bold float-right mt-4 shadow-none w-50 SOISecondayInfo servicesBooking-ConfirmBtn"  id="SOI-thirdConfirm">Next</button>
                <!-- fourth form btns -->
                <button onclick="paymentDetailsBack()" class="btn float-left mt-4 shadow-none btn-secondary border-0 w-25 bg-white text-dark shadow-lg SOISecondayInfo servicesBooking-BackBtn" id="SOI-thirdBack">Back</button>
                <button onclick="document.getElementById('appointmentsForm').submit()" class="btn text-light font-weight-bold float-right mt-4 shadow-none w-50 SOISecondayInfo servicesBooking-ConfirmBtn"  id="SOI-fourthConfirm" style="background-color: #1A936F !important;">Confirm</button>
                <br>
                <br>
            </div>
        </div>
        <div class="col-12 col-md-3 mt-4 m-md-0 p-0">
            <div class="card w-100 bg-ivory m-0 border-0 h-100 servicesBooking-featuredCard font-primary">
                <h4 class="blog-featuredCard-header servicesBooking-featuredCard-header mt-4 mb-3">Featured Blog</h4>
